{
    "name": "jorisvanw/agent",
    "description": "A PHP desktop/mobile user agent parser with support for Laravel, based on Mobiledetect",
    "keywords": ["laravel", "useragent", "agent", "user agent", "browser", "platform", "mobile"],
    "homepage": "https://github.com/jorisvanw/agent",
    "license" : "MIT",
    "authors": [
	{
            "name": "Joris van Willigen",
            "homepage": "http://jorisvanw.com"
        },
        {
            "name": "Jens Segers",
            "homepage": "https://jenssegers.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "^4.0|^5.0",
        "mobiledetect/mobiledetectlib": "^2.0",
        "jaybizzle/crawler-detect" :"1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "satooshi/php-coveralls": "^0.6"
    },
    "autoload": {
        "psr-4": {
            "Jorisvanw\\Agent\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
